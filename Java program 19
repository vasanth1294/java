import java.util.*;
public class table{
public static void main(String args[]){
try{
int n;
Scanner obj=new Scanner(System.in);
System.out.println("enter the number : ");
n=obj.nextInt();
for(int i=1;i<=10;i++){
System.out.println(n+"x"+i+"="+n*i);
}
}
catch(Exception s){
System.out.println("invalid input!!!");
}
}
}
